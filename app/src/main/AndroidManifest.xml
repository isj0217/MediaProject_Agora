<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mediaproject_agora">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".src.ApplicationClass"
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:icon="@drawable/logo_agora_140px"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="n">
        <activity android:name=".src.sign_in.SignInActivity"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".src.main.MainActivity" />
        <activity android:name=".src.sign_up.SignUpActivity" />
        <activity android:name=".src.main.DepartmentBoardActivity" />
        <activity android:name=".src.main.InPostActivity" />
        <activity android:name=".src.main.WritingDepartmentActivity" />
        <activity android:name=".src.main.WritingUsedProductDepartmentActivity" />
        <activity android:name=".src.main.FixingDepartmentActivity" />
        <activity android:name=".src.main.InMessageRoomActivity" />

    </application>

</manifest>